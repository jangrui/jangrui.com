{
  "name": "Linux-Ops-Notes",
  "version": "1.0.0",
  "description": "我要的是一份文档，你却给了我一本书。",
  "scripts": {
    "serve": "vuepress dev docs",
    "cleanServe": "vuepress dev docs --no-cache",
    "build": "vuepress build docs",
    "super:build": "node --max_old_space_size=8192 --unhandled-rejections=strict ./node_modules/vuepress/cli.js build docs",
    "lint": "prettier --check --write .",
    "lint:check": "prettier --check .",
    "lint-md": "markdownlint **/*.md --ignore node_modules --fix",
    "lint-md:check": "markdownlint **/*.md --ignore node_modules",
    "lint-md:style": "remark --quiet .",
    "lint-md:wording": "textlint ./docs/**/*.md",
    "lint-md:wording:fix": "textlint ./docs/**/*.md --fix",
    "comsol:get-words": "node script/comsol/get-words.js",
    "test": "echo \"Warning: no test specified\" && exit 0"
  },
  "author": "Jangrui",
  "license": "MIT",
  "keywords": [
    "Jangrui",
    "Linux",
    "Docker"
  ],
  "dependencies": {},
  "devDependencies": {
    "vuepress": "^1.8.0",
    "vuepress-theme-hope": "^1.9.3"
  }
}
